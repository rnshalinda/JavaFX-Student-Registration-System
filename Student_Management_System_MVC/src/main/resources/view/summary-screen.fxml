<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.icet.sms.controller.SummaryFormController">
   <children>
      <Label layoutX="14.0" layoutY="8.0" text="Summary">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TableView fx:id="tbl_stuDetails" layoutX="23.0" layoutY="54.0" prefHeight="237.0" prefWidth="554.0">
        <columns>
          <TableColumn fx:id="col_Name" prefWidth="184.0" text="Full Name" />
          <TableColumn fx:id="col_Email" prefWidth="162.0" text="Email" />
            <TableColumn fx:id="col_Gender" prefWidth="69.0" text="Gender" />
            <TableColumn fx:id="col_Course" prefWidth="138.0" text="Course" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btn_back" layoutX="318.0" layoutY="318.0" mnemonicParsing="false" onAction="#btnBackToRegistrationAction" text="Back" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button defaultButton="true" layoutX="248.0" layoutY="318.0" mnemonicParsing="false" onAction="#tblReloadAction" text="Reload" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btn_logout" layoutX="515.0" layoutY="356.0" mnemonicParsing="false" onAction="#logoutAction" text="Logout" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
